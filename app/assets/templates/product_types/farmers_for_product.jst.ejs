<h1> FARMERS FOR <%= pt.escape('name') %> </h1>
<button class="leaveMarket">LEAVE THIS MARKET</button>
<ul class="connected-lists">
  <% ptFarmers.each(function(farmer){ %>
    <li>
      <div class='farmer-thumbnail'>
        <div class='thumbnail-product'>
        <%=pt.escape('name') %>
        <div>
        <div class='thumbnail-fname'>
          <a href="#farmers/<%= farmer.escape('name') %>"><%= farmer.escape('name') %></a>
        </div>
      </div>
  <% })%>
</ul>