<ul class='nav nav-tabs nav-justified'>
  <% product_types.each(function(product_type){ %>  
		<% if(product_types.indexOf(product_type) == Math.floor(product_types.length/2)){ %>
		</ul><ul class='nav nav-tabs nav-justified'>
		<% } %>
        <li class="<%=product_type.escape('name')%>"
		data-type="<%=product_type.escape('name')%>">
	       <a class='plz'> <%=product_type.escape('name')%> </a>
        </li>
  <% }) %>
</ul>


<div class='map-canvas' id="index-map">
<p class='disclaimer'>Ahoy, Please be patient, we are loading lots of data and it may take a few moments </p>
</div>
<div class='right-of-map'>
  <form class='zip-form'>
    <label> Change Location
      <input id='autocomplete' type='text'></input>
    </label>
  </form>
  <div class='show-area'>
	<% Mrkts.each(function (market) { %>
	  <div class='market-tab' data-marketname="<%=market.escape('id')%>">
		  <a href=<%="#mrkts/"+ market.escape('id')%>>
				<%=market.escape('marketname') %>
			</a>
		</div>
	<% }) %>
	</div>
</div>